{
  "dashboard": {
    "id": null,
    "title": "Bittensor Inference Subnet - Proxy Server",
    "tags": ["bittensor", "subnet", "inference", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "bittensor_subnet_uptime_seconds",
            "legendFormat": "Uptime (hours)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "basic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bittensor_subnet_total_requests[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "bittensor_subnet_cache_hit_rate",
            "legendFormat": "Cache Hit Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Task Creation Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bittensor_subnet_total_tasks[5m])",
            "legendFormat": "Tasks/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Miner Response Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bittensor_subnet_total_miner_responses[5m])",
            "legendFormat": "Responses/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Database Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bittensor_subnet_database_operations[5m])",
            "legendFormat": "DB Ops/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bittensor_subnet_errors[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "bittensor_subnet_total_requests",
            "legendFormat": "Total Requests"
          },
          {
            "expr": "bittensor_subnet_total_tasks",
            "legendFormat": "Total Tasks"
          },
          {
            "expr": "bittensor_subnet_total_miner_responses",
            "legendFormat": "Total Responses"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
}
